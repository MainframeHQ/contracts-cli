{
  "name": "contracts-cli",
  "version": "0.1.7",
  "main": "lib/index.js",
  "author": "Mainframe",
  "description": "Command line tool for interacting with Ethereum smart contracts",
  "repository": "git@github.com:MainframeHQ/contracts-cli.git",
  "license": "MIT",
  "preferGlobal": true,
  "bin": "./lib/index.js",
  "files": [
    "lib/*",
    "config.json",
    "abi/*"
  ],
  "scripts": {
    "clean": "del lib",
    "build:js": "babel src --out-dir lib",
    "build:flow": "flow-copy-source src lib",
    "build": "yarn clean && yarn build:js && yarn build:flow",
    "lint": "eslint src",
    "test:types": "flow check",
    "test": "yarn lint && yarn test:types",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@ledgerhq/hw-transport-node-hid": "^4.22.0",
    "@ledgerhq/web3-subprovider": "^4.21.0",
    "chalk": "^2.4.0",
    "commander": "^2.17.1",
    "config": "^2.0.1",
    "csv-parser": "^1.12.1",
    "eth-ens-namehash": "^2.0.8",
    "fs-extra": "^5.0.0",
    "got": "^8.3.2",
    "inquirer": "^5.2.0",
    "truffle-hdwallet-provider": "^0.0.3",
    "truffle-hdwallet-provider-privkey": "^0.2.0",
    "web3": "1.0.0-beta.36",
    "web3-provider-engine": "^14.0.6",
    "yargs": "^12.0.1"
  },
  "yargs": {
    "parse-numbers": false
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "del-cli": "^1.1.0",
    "eslint": "^5.3.0",
    "eslint-config-mainframe": "^1.4.0",
    "flow-bin": "^0.79.1",
    "flow-copy-source": "^2.0.2",
    "prettier": "^1.14.0"
  }
}
